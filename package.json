  "main": "dist/loaderify.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "npm run tsc && mocha",
    "tsc": "tsc",
    "prepublish": "npm run clean && npm install --ignore-scripts && npm run tsc"
  },
  "keywords": [
    "browserify-transform",
    "loader"
  ],
  "author": "swimmadude66",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimmadude66/loaderify.git"
  },
  "bugs": {
    "url": "https://github.com/swimmadude66/loaderify/issues"
  },
  "homepage": "https://github.com/swimmadude66/loaderify#readme",
  "dependencies": {
    "async": "^2.1.4",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^7.0.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "typescript": "^2.3.2"
  },
  "peerDependencies": {
    "browserify": "^13.1.1"
  }
}
